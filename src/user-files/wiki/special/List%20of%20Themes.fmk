[!scheme
(define (render-theme-pages wiki-files)
  (if (eqv? wiki-files '())
    ""
    (let ((wiki-file (car wiki-files)))
      (if (eqv? (indexOf wiki-file "themes/") 0)
        (<< wiki-file "\n" (render-theme-pages  (cdr wiki-files)))
        (render-theme-pages (cdr wiki-files))))))


(let ((wiki-files (collect-wiki-page-list)))
(let ((theme-list (render-theme-pages wiki-files)))
  (fronkenmark (<< "[menu "
    theme-list
      " menu]"
) #f #f)))

scheme!]
